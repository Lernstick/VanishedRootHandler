<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox id="main-vbox" stylesheets="@mainScene.css" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ch.lernstick.vanishedroothandler.view.javafx.MainSceneController">
    <children>
        <Label id="error-label" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="%Vanished_Root_Label" VBox.vgrow="ALWAYS">
            <VBox.margin>
                <Insets left="30.0" right="30.0" />
            </VBox.margin>
        </Label>
        <HBox id="button-hbox" maxWidth="1.7976931348623157E308" VBox.vgrow="ALWAYS">
            <children>
                <Button id="reboot-button" fx:id="rebootButton" defaultButton="true" mnemonicParsing="false" text="%Reboot" />
                <Button id="poweroff-button" fx:id="powerOffButton" mnemonicParsing="false" text="%Power_Off" />
            </children>
        </HBox>
    </children>
</VBox>
